{
  "name": "sackobaqatar.com",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "bun@1.3.3",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "copy-images": "node scripts/copy-event-images.sh",
    "generate-event-data": "node scripts/generate-complete-event-data.js",
    "optimize-images": "node scripts/optimize-images.js",
    "setup-images": "bun run copy-images && bun run generate-event-data && bun run optimize-images",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:edge": "cypress run --browser edge",
    "test:e2e": "cypress run",
    "test:e2e:headed": "cypress run --headed",
    "test:e2e:dev": "start-server-and-test dev http://localhost:5173 cypress:run",
    "test:accessibility": "cypress run --spec 'cypress/e2e/12-accessibility.cy.ts'",
    "test:performance": "cypress run --spec 'cypress/e2e/13-performance.cy.ts'",
    "test:integration": "cypress run --spec 'cypress/e2e/14-integration.cy.ts'",
    "test:responsive": "cypress run --spec 'cypress/e2e/11-responsive-design.cy.ts'"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.87.0",
    "@types/react-lazy-load-image-component": "^1.6.5",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.556.0",
    "react": "^19.2.1",
    "react-country-flag": "^3.1.0",
    "react-dom": "^19.2.1",
    "react-feather": "^2.0.10",
    "react-icons": "^5.5.0",
    "react-lazy-load-image-component": "^1.6.3",
    "react-masonry-css": "^1.0.16",
    "react-router-dom": "^7.10.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.22",
    "axe-core": "^4.11.0",
    "cypress": "^15.7.1",
    "cypress-axe": "^1.7.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "webpack": "^5.103.0"
  }
}
